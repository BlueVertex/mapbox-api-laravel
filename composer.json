{
    "name": "bluevertex/mapbox-api-laravel",
    "description": "A Laravel 5+ Package for managing Mapbox Datasets and Tilesets",
    "type": "library",
    "require": {
        "php": ">=7.0.0",
        "illuminate/contracts": "^5.5",
        "illuminate/support": "^5.5",
        "kitetail/zttp": "^0.3.0",
        "jmikola/geojson": "^1.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Matt Fox",
            "email": "matt.fox@thebluevertex.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "BlueVertex\\MapBoxAPILaravel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BlueVertex\\MapBoxAPILaravel\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BlueVertex\\MapBoxAPILaravel\\MapBoxAPILaravelServiceProvider"
            ],
            "aliases": {
                "Mapbox": "BlueVertex\\MapBoxAPILaravel\\Facades\\Mapbox"
            }
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "require-dev": {
        "orchestra/testbench": "~3.5",
        "phpunit/phpunit": "^6.5",
        "codedungeon/phpunit-result-printer": "^0.4.4"
    }
}
